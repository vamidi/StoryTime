{
	"compileOnSave": false,
	"compilerOptions": {
		"importHelpers": true,
		"module": "esnext",
		"baseUrl": "./src",
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"target": "es2015",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2017",
			"dom"
		],
		"plugins": [
			{
				"name": "tslint-language-service"
			}
		],
		"paths": {
			"@angular/*": [
				"../node_modules/@angular/*"
			],
			"@nebular/*": [
				"../node_modules/@nebular/*"
			],
			"@app-theme/*": [
				"app/_theme/*"
			],
			"@app-core/*": [
				"app/_core/*"
			],
			"@app-dashboard/*": [
				"app/dashboard/*"
			]
		}
	}
}
